{
    "name": "Stackoverflow search \"I'm feeling lucky\"",
    "description": "Allows to see best answer to given question on stackoverflow.com (Like Google's \"I'm feeling lucky\" function)",
    "version": "1.0.0",
    "manifest_version": 2,
    "default_locale": "en",
    "permissions": [
        "https://stackoverflow.com/"
    ],
    "browser_action": {
        "default_icon": "img/icon48.png",
        "default_popup": "html/search-popup.html",
        "default_title": "Stackoverflow I'm feeling lucky"
    },
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    }
}
